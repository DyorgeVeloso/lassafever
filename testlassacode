import pandas as pd
import matplotlib as plt
import numpy as np

data = pd.read_csv('convertcsv.csv')


data.head()

data.columns.values

newdata = ['Country', 'Number_acutecases']
doencas = data.filter(items=newdata)

type(doencas)

doencas.dropna(inplace = True)
D1 = doencas['Country'].astype(float)
D2 = doencas['Number_acutecases'].astype(float)

plt.plot(D1, D2)
plt.show()

